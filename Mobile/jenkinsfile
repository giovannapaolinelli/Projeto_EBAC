pipeline {
    agent any

    stages {
        stage('Clonar o repositorio') {
            steps {
                git branch: 'main', url: 'https://github.com/giovannapaolinelli/EBAC-Mobile-WDIO.git'
            }
        }
        stage('Instalar dependencias'){
            steps {
                bat 'npm install'
            }
        }
        stage('Executar Testes'){
            steps {
                bat 'npm test'
            }
        }
    }
}